<% c = Content.find(locals[:content_id])%>
<tr id="content_<%= c.id %>">
	<td><div class="little-img-container"><%= image_tag(c.image) %></div></td>
	<td class="index"><%= link_to c.name, c.url , :target => "_blank"%></td>
	<td>


		<!-- delete content icon cross -->
		<% if current_member && current_member.is_admin %>
			<div class="little-img-container delete-icon-div">
				<%= link_to image_tag("delete_icon.png"), controller: "home", action: "delete_content",remote: true, content_id: c.id %>
			</div>
		<% end %>



	</td>
</tr>